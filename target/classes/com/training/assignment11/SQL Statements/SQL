//Add_student procedure
create or replace PROCEDURE ADD_STUDENT (ST_ID IN VARCHAR2, ST_NM IN VARCHAR2, ADDRESS IN VARCHAR2, BR IN VARCHAR2)
IS
BEGIN
    INSERT INTO STUDENT VALUES(ST_ID, ST_NM, ADDRESS, BR);
END;

//Student details function
create or replace PROCEDURE ADD_STUDENT (ST_ID IN VARCHAR2, ST_NM IN VARCHAR2, ADDRESS IN VARCHAR2, BR IN VARCHAR2)
IS
BEGIN
    INSERT INTO STUDENT VALUES(ST_ID, ST_NM, ADDRESS, BR);
END;